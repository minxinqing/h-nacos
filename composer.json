{
    "name": "minxinqing/nacos",
    "description": "A PHP implementation of Nacos OpenAPI, particularly adapt for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "nacos"
    ],
    "type": "library",
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Nacos\\": "src/"
        }
    },
    "require": {
        "hyperf/contract": "~2.0.0",
        "hyperf/guzzle": "~2.0.0",
        "hyperf/load-balancer": "~2.0.0",
        "hyperf/utils": "~2.0.0"
    },
    "require-dev": {
    },
    "suggest": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "hyperf/event": "~2.0.0",
        "hyperf/process": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/logger": "~2.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Nacos\\ConfigProvider"
        }
    }
}
